			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "customer",
								"type": "DatasetReference"
							},
							"name": "Customer"
						}
					],
					"sinks": [
						{
							"dataset": {
								"referenceName": "AzureSqlTable1",
								"type": "DatasetReference"
							},
							"name": "sinkinsql"
						}
					],
					"transformations": [
						{
							"name": "removrenullwithunknown"
						},
						{
							"name": "DeleteNullrecord"
						},
						{
							"name": "sortaccbal"
						}
					],
					"scriptLines": [
						"source(output(",
						"          C_CUSTKEY as integer,",
						"          C_NAME as string,",
						"          C_ADDRESS as string,",
						"          C_NATIONKEY as integer,",
						"          C_PHONE as string,",
						"          C_ACCTBAL as double,",
						"          C_MKTSEGMENT as string,",
						"          C_COMMENT as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false) ~> Customer",
						"DeleteNullrecord derive(C_NAME = iifNull(C_NAME, 'unknown', C_NAME)) ~> removrenullwithunknown",
						"Customer alterRow(deleteIf(isNull(C_CUSTKEY))) ~> DeleteNullrecord",
						"removrenullwithunknown sort(desc(C_ACCTBAL, true)) ~> sortaccbal",
						"sortaccbal sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     input(",
						"          custkey as integer,",
						"          cname as string,",
						"          caddress as string,",
						"          cnation as integer,",
						"          cphone as string,",
						"          caccbal as double,",
						"          cmanagement as string,",
						"          ccomminsion as string",
						"     ),",
						"     deletable:false,",
						"     insertable:true,",
						"     updateable:false,",
						"     upsertable:false,",
						"     format: 'table',",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true,",
						"     errorHandlingOption: 'stopOnFirstError',",
						"     mapColumn(",
						"          custkey = C_CUSTKEY,",
						"          cname = C_NAME,",
						"          caddress = C_ADDRESS,",
						"          cnation = C_NATIONKEY,",
						"          cphone = C_PHONE,",
						"          caccbal = C_ACCTBAL,",
						"          cmanagement = C_MKTSEGMENT,",
						"          ccomminsion = C_COMMENT",
						"     )) ~> sinkinsql"
					]
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/customer')]",
				"[concat(variables('factoryId'), '/datasets/AzureSqlTable1')]"
			]
		}
	]
